MSC_WARNING_LEVEL=/W3 /WX
!ENDIF

!IF "$(DDKBUILDENV)" == "fre" || "$(DDKBUILDENV)" == "free"
MSC_OPTIMIZATION=$(MSC_OPTIMIZATION) /Ob2 /O1
!endif

INCLUDES=$(DDK_INC_PATH);$(WDM_INC_PATH)
INCLUDES=$(INCLUDES);..\..\Device;



